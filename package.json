{
  "name": "elm-starter",
  "version": "1.0.0",
  "description": "Elm Taco Example",
  "main": "server.js",
  "scripts": {
    "test": "xo",
    "lint": "xo --fix",
    "dev:client": "elm-live ./client/Main.elm --debug --dir=./public --output ./public/main.bundle.js",
    "dev:server": "NODE_ENV=development PORT=5000 nodemon --watch ./api ./server.js | pino-colada",
    "start:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "prod:client": "npm run browserify:prod && elm-make ./client/Main.elm --yes --output ./public/main.bundle.js && uglifyjs2 ./public/main.bundle.js -o ./public/main.bundle.js",
    "browserify:dev": "NODE_ENV=development browserify ./js -t envify -o ./public/bundle.js",
    "browserify:prod": "NODE_ENV=production browserify ./js -t envify -g uglifyify -o ./public/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abradley2/elm-starter.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/abradley2/elm-starter/issues"
  },
  "homepage": "https://github.com/abradley2/elm-starter#readme",
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "semicolon": false,
    "space": 2
  },
  "devDependencies": {},
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "browserify": "^15.2.0",
    "co": "^4.6.0",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "cuid": "^2.0.2",
    "elm": "^0.18.0",
    "elm-live": "^2.7.5",
    "envify": "^4.1.0",
    "express": "^4.15.2",
    "express-fileupload": "^0.4.0",
    "ioredis": "^3.2.2",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.1.0",
    "nodemon": "^1.11.0",
    "pino": "^4.0.3",
    "pino-colada": "^1.0.3",
    "shortid": "^2.2.8",
    "uglify-js": "^2.8.12",
    "uglify-js2": "^2.1.11",
    "uglifyify": "^4.0.5",
    "uuid": "^3.2.1",
    "xo": "^0.17.1"
  }
}
