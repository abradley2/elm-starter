{
  "name": "elm-starter",
  "version": "1.0.0",
  "description": "Elm Taco Example",
  "main": "server.js",
  "scripts": {
    "test": "xo",
    "dev:client": "elm-live ./client/Main.elm --dir=./public --output ./public/main.bundle.js",
    "dev:server": "NODE_ENV=development PORT=5000 nodemon --watch ./api ./server.js | pino-colada",
    "start:dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "prod:client": "elm-make ./client/Main.elm --output ./public/main.bundle.js",
    "prod:server": "NODE_ENV=production PORT=5000 node ./server.js",
    "start:prod": "npm run prod:client && npm run prod:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abradley2/elm-starter.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/abradley2/elm-starter/issues"
  },
  "homepage": "https://github.com/abradley2/elm-starter#readme",
  "xo": {
    "space": 4
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "elm-live": "^2.7.5",
    "nodemon": "^1.11.0",
    "uglify-js": "^2.8.12",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "express": "^4.15.2",
    "pino": "^4.0.3",
    "pino-colada": "^1.0.3"
  }
}
